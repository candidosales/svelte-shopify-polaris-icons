{
  "name": "@candidosales/svelte-shopify-polaris-icons",
  "version": "0.0.13",
  "description": "Svelte Shopify Polaris Icons",
  "author": {
    "name": "Candido Sales Gomes",
    "email": "candidosg@gmail.com",
    "url": "https://candidosales.me"
  },
  "bugs": "https://github.com/candidosales/svelte-shopify-polaris-icons/issues",
  "homepage": "https://svelte-shopify-polaris-icons.vercel.app/",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "write-package-json": "node ./scripts/package-json-writer.js",
    "add-exports": "node ./scripts/add-exports-to-package-json.js",
    "package:publish": "standard-version && git push --follow-tags origin main && npm run package && npm run add-exports && npm run write-package-json && npm publish"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^4.0.0",
    "tailwind-merge": "^1.13.2",
    "tailwindcss": "^3.3.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.36.2",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.22.3",
    "@sveltejs/package": "^2.2.0",
    "@tailwindcss/typography": "^0.5.9",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.1",
    "publint": "^0.2.0",
    "sass": "^1.64.1",
    "svelte": "^4.1.1",
    "svelte-check": "^3.4.6",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vitest": "^0.33.0"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "keywords": [
    "svelte",
    "sveltekit",
    "shopify",
    "polaris",
    "icons",
    "svg"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/candidosales/svelte-shopify-polaris-icons/"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./AbandonedCartFilledMajor.svelte": {
      "types": "./dist/AbandonedCartFilledMajor.svelte.d.ts",
      "svelte": "./dist/AbandonedCartFilledMajor.svelte"
    },
    "./AbandonedCartMajor.svelte": {
      "types": "./dist/AbandonedCartMajor.svelte.d.ts",
      "svelte": "./dist/AbandonedCartMajor.svelte"
    },
    "./AccessibilityMajor.svelte": {
      "types": "./dist/AccessibilityMajor.svelte.d.ts",
      "svelte": "./dist/AccessibilityMajor.svelte"
    },
    "./ActivitiesMajor.svelte": {
      "types": "./dist/ActivitiesMajor.svelte.d.ts",
      "svelte": "./dist/ActivitiesMajor.svelte"
    },
    "./AddCodeMajor.svelte": {
      "types": "./dist/AddCodeMajor.svelte.d.ts",
      "svelte": "./dist/AddCodeMajor.svelte"
    },
    "./AddImageMajor.svelte": {
      "types": "./dist/AddImageMajor.svelte.d.ts",
      "svelte": "./dist/AddImageMajor.svelte"
    }
  },
  "license": "MIT"
}
